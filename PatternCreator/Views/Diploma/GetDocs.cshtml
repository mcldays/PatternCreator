@model  Dictionary<int, string>
@{
    IEnumerable<int> pageCount = ViewBag.PageCount;
    int currentPage = ViewBag.CurrentPage;
    string protocol = ViewBag.Protocol;
    int currentPart = currentPage % 5 == 0 ? (int)Math.Truncate((decimal)currentPage / 5) - 1 : (int)Math.Truncate((decimal)currentPage / 5);
    int parts = pageCount.Count() % 5 == 0 ? (int)Math.Truncate((decimal)pageCount.Count() / 5) - 1 : (int)Math.Truncate((decimal)pageCount.Count() / 5);
    int i = 1;
}
<h5>Выберите редактируемые документы:</h5>
<table id="DocsTable" class="table text-center">
    <thead>
        <tr>
            <th>№ п/п</th>
            <th>№ документа</th>
            <th>№ протокола</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var doc in Model)
        {
            <tr data-docid="@(doc.Key)" class="docrow">
                <td>@(i++)</td>
                <td>@(doc.Key)</td>
                <td>@(doc.Value)</td>
            </tr>
        }
    </tbody>
</table>
@if (pageCount.Count() > 1)
{
    <div class="row justify-content-center">
        @if (currentPage > 5)
        {
            int pg = (currentPart - 1) * 5 + 1;
            <div class="col-auto mx-1 p-0">
                <a class="btn btn-primary" data-ajax="true" data-ajax-failure="alert('Что то пошло не так! попробуйте ещё раз')" href="/Diploma/GetDocsByProtocol?protocolname=@(protocol)&page=@(pg)" data-ajax-begin="$('#preloader-id').removeClass('done');" data-ajax-complete="$('#preloader-id').addClass('done');" data-ajax-method="POST" data-ajax-update="#targetContainer"><<</a>
            </div>
        }
        @if (currentPage > 1)
        {
            <div class="col-auto mx-1 p-0">
                <a class="btn btn-primary" data-ajax="true" data-ajax-failure="alert('Что то пошло не так! попробуйте ещё раз')" href="/Diploma/GetDocsByProtocol?protocolname=@(protocol)&page=@(currentPage-1)" data-ajax-begin="$('#preloader-id').removeClass('done');" data-ajax-complete="$('#preloader-id').addClass('done');" data-ajax-method="POST" data-ajax-update="#targetContainer"><</a>
            </div>
        }
        @foreach (int page in pageCount.Skip(5 * currentPart).Take(5))
        {
            <div class="col-auto mx-1 p-0">
                <a class="btn btn-outline-primary @(page==currentPage?"active":"")" data-ajax="true" data-ajax-failure="alert('Что то пошло не так! попробуйте ещё раз')" href="/Diploma/GetDocsByProtocol?protocolname=@(protocol)&page=@(currentPage-1)" data-ajax-begin="$('#preloader-id').removeClass('done');" data-ajax-complete="$('#preloader-id').addClass('done');" data-ajax-method="POST" data-ajax-update="#targetContainer">@(page)</a>
            </div>
        }
        @if (!(currentPage > parts * 5))
        {
            int pg = (currentPart + 1) * 5 + 1;
            <div class="col-auto mx-1 p-0">
                <a class="btn btn-primary" data-ajax="true" data-ajax-failure="alert('Что то пошло не так! попробуйте ещё раз')" href="/Diploma/GetDocsByProtocol?protocolname=@(protocol)&page=@(pg)" data-ajax-begin="$('#preloader-id').removeClass('done');" data-ajax-complete="$('#preloader-id').addClass('done');" data-ajax-method="POST" data-ajax-update="#targetContainer">>></a>
            </div>
        }
        @if (currentPage < pageCount.Last())
        {
            <div class="col-auto mx-1 p-0">
                <a class="btn btn-primary" data-ajax="true" data-ajax-failure="alert('Что то пошло не так! попробуйте ещё раз')" href="/Diploma/GetDocsByProtocol?protocolname=@(protocol)&page=@(currentPage+1)" data-ajax-begin="$('#preloader-id').removeClass('done');" data-ajax-complete="$('#preloader-id').addClass('done');" data-ajax-method="POST" data-ajax-update="#targetContainer">></a>
            </div>
        }

    </div>

}
<div class="row justify-content-center my-3">
        <button id="backProtocol" class="btn btn-primary mr-auto">Назад</button>
    
        <button id="redactDocs" class="btn btn-primary mx-1" disabled>Редактировать</button>
        <button id="deleteDocs" class="btn btn-primary" disabled>Удалить</button>
</div>
<script>
    $(document).ready(function () {
        $("#DocsTable").TableSelection({
            status: 'multiple'
        });
    });
</script>
